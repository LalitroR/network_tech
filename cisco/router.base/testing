! On router1

vlan 20
 name test

ip route vrf RED 1.1.1.0 255.255.255.0 2.2.2.2


show run interface Vlan 1
default interface Vlan 1

ip vrf asdf


vrf definition RED
	description RED VRF
	no rd 1:2
	rd 1:2
	export map route-map-name
	route-target both 1:2

route-map blah_blah permit 10
 set local-preference 220


show vlan id 604 | include Po10

ip access-list extended ACL-NAME
  permit ip any any
  no permit ip any any

ip prefix-list NAME seq 10 permit 10.225.6.0/24
ip prefix-list NAME seq 10 deny 10.225.6.0/24
ip prefix-list NAME seq 10 permit 10.225.6.0/24 ge 1
ipv6 prefix-list NAME seq 10 permit ::/0
ipv6 prefix-list NAME seq 200 deny ::/0 ge 1

router bgp 123
	address-family ipv4 vrf VRF-NAME
		network 1.1.1.0
		network 1.1.1.0 mask 255.255.255.0
		neighbor 1.1.1.1 activate
		neighbor 1.1.1.1 shutdown
		neighbor 1.1.1.1 ttl-security hops 1
		neighbor 1.1.1.1 version 4
		neighbor 1.1.1.1 timers 1 2
		neighbor 1.1.1.1 remote-as 20
		neighbor 1.1.1.1 route-map ROUTE-MAP-NAME in
		neighbor 1.1.1.1 route-map ROUTE-MAP-NAME out
		neighbor 1.1.1.1 inherit peer-policy POLICY
	exit-address-family
	address-family ipv6 vrf VRF-NAME
		network ::/0
		neighbor ::1 activate
		neighbor ::1 shutdown
		neighbor ::1 remote-as 1
		neighbor ::1 ttl-security hops 1
		neighbor ::1 version 4
		neighbor ::1 timers 1 2
		neighbor ::1 route-map  ROUTE-MAP-NAME in
		neighbor ::1 route-map  ROUTE-MAP-NAME out
		neighbor ::1 inherit peer-policy POLICY
	exit-address-family


configure terminal
